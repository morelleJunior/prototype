{
  "rewrites": [
    {
      "source": "/api/:path*",  
      "destination": "https://taiffapi.avlq.online/:path*" 
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
